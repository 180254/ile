          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "with\n    zigbee2mqtt_sonoff_s60zbtpf_b as (\n        SELECT\n            timestamp AS ts,\n            lag(timestamp) OVER (PARTITION BY ieee_address ORDER BY timestamp) AS prev_ts,\n            power,\n        FROM zigbee2mqtt_sonoff_s60zbtpf\n        WHERE ${filter:raw} and $__timeFilter(timestamp)\n    ),\n    interval_s as (\n        select (case\n            when '$interval' like '%s' then cast(left('$interval', -1) as int)*1\n            when '$interval' like '%m' then cast(left('$interval', -1) as int)*60\n            when '$interval' like '%h' then cast(left('$interval', -1) as int)*3600\n            when '$interval' like '%d' then cast(left('$interval', -1) as int)*86400\n            when '$interval' like '%w' then cast(left('$interval', -1) as int)*604800\n            when '$interval' like '%M' then cast(left('$interval', -1) as int)*2678400\n            when '$interval' like '%y' then cast(left('$interval', -1) as int)*31536000\n            else 0 end\n        ) as value\n    ),\n    consumption_t as (\n        (\n            select\n                t1.ts,\n                sum(t1.power*(datediff('u', t1.ts, t1.prev_ts)))/(3600.0*1000000) as consumption\n            from zigbee2mqtt_sonoff_s60zbtpf_b t1\n            cross join interval_s\n            where datediff('s', t1.ts, t1.prev_ts) <= interval_s.value\n        ) timestamp(ts)\n    )\nselect\n    $__time(ts),\n    sum(consumption) as value\nfrom consumption_t\nsample by $interval fill(null) align to calendar",
